{
  "extends": [
    "airbnb",
    "plugin:flowtype/recommended",
    "plugin:jest/recommended"
  ],
  "plugins": [
    "flowtype"
  ],
  "rules": {
    "function-paren-newline": "off",
    "jsx-a11y/anchor-is-valid": "warn",
    "no-confusing-arrow": "off",
    "react/jsx-filename-extension": "off"
  }
}
